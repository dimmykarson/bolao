<p id="notice"><%= notice %></p>

<h1>Apostaitems</h1>

<table>
  <thead>
    <tr>
      <th>Aposta</th>
      <th>Colocacao</th>
      <th>Clube</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @apostaitems.each do |apostaitem| %>
      <tr>
        <td><%= apostaitem.aposta %></td>
        <td><%= apostaitem.colocacao %></td>
        <td><%= apostaitem.clube %></td>
        <td><%= link_to 'Show', apostaitem %></td>
        <td><%= link_to 'Edit', edit_apostaitem_path(apostaitem) %></td>
        <td><%= link_to 'Destroy', apostaitem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Apostaitem', new_apostaitem_path %>
